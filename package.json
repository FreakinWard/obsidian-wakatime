{
  "name": "obsidian-wakatime",
  "version": "0.1.0",
  "description": "Obsidian plugin for WakaTime",
  "main": "src/main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "build:local": "rollup --config rollup.local.config.js --environment BUILD:production",
    "watch": "npm-watch"
  },
  "watch": {
    "build:local": {
      "silent": false,
      "patterns": [
        "",
        "src"
      ],
      "extensions": "ts"
    }
  },
  "keywords": [
    "obsidian",
    "wakatime",
    "time-tracking",
    "metrics"
  ],
  "author": "FreakinWard",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/adm-zip": "^0.4.34",
    "@types/node": "^14.14.37",
    "@types/request": "^2.48.7",
    "@types/rimraf": "^3.0.2",
    "npm-watch": "^0.11.0",
    "rollup": "^2.32.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.6",
    "fs": "0.0.1-security",
    "obsidian": "^0.12.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "which": "^2.0.2"
  }
}
